<h4>
  Languages:
  <br/>
  <% Language.all.each do |language| %>
  > <%= language.language %> <br/>
  <% end %>
</h4>

<h4>
  Contacts:
  <br/>
  <% Contact.all.each do |contact| %>
  > <%= contact.name %> <br/>
  <% end %>
</h4>

<h4>
  Links:
  <br/>
  <% RelevantLink.all.each do |link| %>
  > <%= link.name %> <br/>
  <% end %>
</h4>

<h4>
  Statements:
  <br/>
  <% Statement.all.each do |statement| %>
  > <%= statement.title %> <br/>
  > <%= statement.text %> <br/>
  <% end %>
</h4>

<h4>
  Topics:
  <br/>
  <% QuestionTopic.all.each do |topic| %>
  > <%= topic.name %> <br/>
  <% end %>
</h4>

<h4>
  Questions:
  <br/>
  <% Question.all.each do |question| %>
  > <%= question.question %> <br/>
  > <%= question.answer %> <br/>
  <% end %>
</h4>

<h4>
  Study:
  <br/>
  <% Study.all.each do |study| %>
  > <%= study.title %> <br/>
  <% end %>
</h4>

<h4>
  Study Fields:
  <br/>
  <% StudyField.all.each do |field| %>
  > <%= field.title %> <br/>
  <% end %>
</h4>

<h4>
  Study Variables:
  <br/>
  <% StudyVariable.all.each do |variable| %>
  > <%= variable.name %>( <%= variable.study_field.title %> ) <br/>
  <% end %>
</h4>
